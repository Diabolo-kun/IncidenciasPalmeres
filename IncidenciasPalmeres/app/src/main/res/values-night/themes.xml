<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.IncidenciasPalmeres" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:screenOrientation">portrait</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimaryNight</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryDarkNight</item>
        <item name="colorOnPrimary">@color/colorAccentNight</item>
        <item name="android:windowBackground">@color/colorBackgroundNight</item>
        <item name="android:textColor">@color/colorTextNight</item>
    </style>


</resources>